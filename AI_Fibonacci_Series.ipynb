      ],
      "source": [
        "# Fibonacci Series:\n",
        "n=int(input(\"enter max range: \"))\n",
        "\n",
        "a=0\n",
        "b=1\n",
        "ref=0\n",
        "lisst=[]\n",
        "lisst.append(a)\n",
        "lisst.append(b)\n",
        "#print(lisst)\n",
        "while ref<n:\n",
        "  sum=a+b\n",
        "  if sum<=n:\n",
        "    lisst.append(a+b)\n",
        "  ref=sum\n",
        "  a=lisst[-2]\n",
        "  b=lisst[-1]\n",
        "print(\"the fibonacci series is: \")\n",
        "print(lisst)\n"
      ]
    }
